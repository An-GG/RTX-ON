{
  "name": "RTX ON",
  "version": "1.0",
  "description": "pen15",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.khanacademy.org/*"],
      "js": ["sidekick.js"],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "popup.png",
    "default_popup": "popup.html",
    "default_title": "do it. just do it."
  },
  "manifest_version": 2
}
